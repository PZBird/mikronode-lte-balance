    "mikronode": "^2.3.11",
    "rxjs": "~5.3.0"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "nodemon": "^1.18.10",
    "prettier": "1.19.1"
